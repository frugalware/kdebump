pkgname=libmemcached
pkgver=1.0.14
pkgrel=1
pkgdesc="A C and C++ client library for memcached"
depends=('memcached' 'cyrus-sasl')
groups=('lib-extra')
archs=('i686' 'x86_64')
_F_launchpad_branch="1.0"
Finclude launchpad
sha1sums=('fe19ffa2bdfe2108f14019bb76adffb7a3bb7eee')

build() {
	Fcd
	Fconf --prefix=/usr
	Fmake
	Fmakeinstall
}

# optimization OK
