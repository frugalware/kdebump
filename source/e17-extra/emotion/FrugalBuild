pkgname=emotion
pkgver=1.7.3
pkgrel=1
pkgdesc="Video playback library for E17"
depends=('edje>=1.7.3' 'xine-lib>=1.2.2-2' 'gstreamer')
groups=('e17-extra' 'e17-core')
archs=('i686' 'x86_64')
Finclude e17
sha1sums=('4f34c2015de761b0b778245bcbc33556817103d8')

build()
{
	Fcd
	Fautoreconf
        Fbuild DESTDIR=$Fdestdir
}

# optimization OK
