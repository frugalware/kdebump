pkgname=emotion
pkgver=1.7.2
pkgrel=1
pkgdesc="Video playback library for E17"
depends=('edje>=1.7.2' 'xine-lib>=1.2.2' 'gstreamer')
groups=('e17-extra' 'e17-core')
archs=('i686' 'x86_64')
Finclude e17
sha1sums=('a20f5a981f5d7ff8c4244091230957d9d4fc57a5')

build()
{
	Fcd
	Fautoreconf
        Fbuild DESTDIR=$Fdestdir
}

# optimization OK
